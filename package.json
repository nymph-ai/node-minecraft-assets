{
  "name": "@nymph-ai/minecraft-assets",
  "version": "1.21.11-nymph.0",
  "description": "Provide minecraft assets (nymph-ai fork)",
  "main": "index.js",
  "tonicExampleFilename": "example.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "standard",
