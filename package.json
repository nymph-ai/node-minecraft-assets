    "prepare": "npm install require-self && require-self",
    "prepack": "git submodule update --init --recursive && node scripts/verify-data.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nymph-ai/node-minecraft-assets"
  },
  "keywords": [
    "minecraft",
    "assets"
  ],
  "author": "Romain Beaumont <romain.rom1@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nymph-ai/node-minecraft-assets/issues"
  },
  "homepage": "https://github.com/nymph-ai/node-minecraft-assets#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "lib/",
    "minecraft-assets/data/**",
    "README.md",
    "LICENSE",
    "example.js"
  ],
  "devDependencies": {
    "require-self": "^0.2.3",
    "standard": "^17.0.0"
  }
}
